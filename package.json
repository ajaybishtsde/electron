{
    "name": "ryver",
    "productName": "Ryver",
    "electronVersion": "6.1.12",
    "version": "1.3.3",
    "versionName": "Daily Dose",
    "description": "Ryver Desktop",
    "main": "./javascripts/browser",
    "author": {
        "name": "Ryver, Inc.",
        "email": "support@ryver.com",
        "url": "http://ryver.com"
    },
    "scripts": {
        "red-marker": "tsc --noEmit --watch --pretty",
        "clean:es": "rimraf out/es",
        "bootstrap": "node script/bootstrap",
        "build:mac": "node script/build --code-sign --compress-artifacts --create-disk-image",
        "build:win": "node script/build --code-sign --compress-artifacts --create-windows-installer",
        "build:win64": "node script/build --code-sign --compress-artifacts --create-windows-installer --arch=x64",
        "build:win:installer":"node msi-build-installer",
        "build:deb": "node script/build --compress-artifacts --create-deb-package",
        "build:rpm": "node script/build --compress-artifacts --create-rpm-package",
        "build:arm": "node script/build --compress-artifacts --arch=arm",
        "watch": "webpack --watch --color --config webpack.config.js --mode development",
        "watch:renderer": "webpack --watch --color --env rendererOnly --config webpack.config.js --mode development",
        "watch:browser": "webpack --watch --color --env browserOnly --config webpack.config.js --mode development",
        "compile": "webpack --color --config webpack.config.js --mode production",
        "compile:es": "yarn clean:es && babel src --extensions .ts,.tsx --out-dir out/es --ignore \\.spec.tsx?$",
        "serve": "cross-env BABEL_ENV=dev-server node script/server",
        "start": "cross-env DEBUG='*,-redux-electron-bridge:*,-remote-event-browser,-rotatelog-stream' DIAGNOSTICS='-winston:*' DEBUG_COLORS=1 node script/run",
        "start:with-dev-server": "cross-env DEBUG='*,-redux-electron-bridge:*,-remote-event-browser,-rotatelog-stream' DIAGNOSTICS='-winston:*' DEBUG_COLORS=1 RYVER_DEVELOPER_SERVER='http://localhost:3200' node script/run",
        "test": "cross-env TS_NODE_PROJECT=./tsconfig.spec.json mocha --require setup-mocha src/**/*.spec.ts || exit 0",
        "lint": "tslint --project tsconfig.json --format stylish --fix || exit 0",
        "i18n:schema": "yarn compile:es && i18n-tag-schema out/es -s locales-schema.json",
        "i18n:export": "i18n-tag-po-tools export -s locales-schema.json locales-schema.po && i18n-tag-po-tools export -i -s locales-schema.json locales/en-US.po",
        "i18n:import": "i18n-tag-po-tools import locales/*.po",
        "i18n:merge": "i18n-tag-po-tools export -m -s locales-schema.json locales/*.po",
        "i18n:update": "yarn i18n:schema && yarn i18n:merge",
        "i18n:check": "i18n-tag-schema locales -v -s locales-schema.json",
        "deb": "node script/deb"
    },
